@model IEnumerable<PatenPottery.ViewModels.OrderListView>

@{
    ViewData["Title"] = "Order List";
}

<h2>Order List</h2>

<table class="table">
    <thead>
        <tr>
            <th>Order Number</th>
            <th>Name</th>
            <th>Number</th>
            <th>Address</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.OrderNumber</td>
                <td>@order.Name</td>
                <td>@order.Number</td>
                <td>@order.Address</td>
                <td>@order.Status</td>
            </tr>
        }
    </tbody>
</table>
